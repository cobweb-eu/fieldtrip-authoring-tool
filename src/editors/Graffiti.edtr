<form id="form373" data-ajax="false">

<div class="fieldcontain" id="fieldcontain-text-1">
<label for="form-text-1">ID</label>
<input name="form-text-1" id="form-text-1" type="text" required="" placeholder="Placeholder" maxlength="10">
</div>

<div class="fieldcontain" id="fieldcontain-select-1">
<fieldset>
<legend>Type</legend>
<select id="form-select-1" required="">
<option value=""></option>
<option value="Tag">Tag</option>
<option value="Picture">Picture</option>
<option value="Stencil">Stencil</option>
<option value="Words">Words</option>
<option value="Other (specify)">Other (specify)</option></select>
</fieldset>
</div>

<div class="fieldcontain" id="fieldcontain-textarea-1">
<label for="form-textarea-1">Other Type</label>
<textarea name="form-textarea-1" id="form-textarea-1" placeholder="only if other" ></textarea>
</div><div class="fieldcontain" id="fieldcontain-range-1" style="position: relative;">
