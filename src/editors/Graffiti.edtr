<input name="form-range-1" id="form-range-1" type="range" required="" step="0.5" min="0" max="10" placeholder="Placeholder" maxlength="10">
</div>

<div class="fieldcontain" id="fieldcontain-select-2" style="position: relative;">
<fieldset>
<legend>Offensive</legend>
<select id="form-select-2" required="">
<option value=""></option>
<option value="Yes">Yes</option>
<option value="No">No</option></select>
</fieldset>
</div><div class="fieldcontain" id="fieldcontain-image-1">
<div class="button-wrapper button-camera">
<input name="form-image-1" id="form-image-1" type="file" accept="image/png" capture="camera" required="" class="camera">
<label for="form-image-1">Photo</label>
</div>
</div>







<div id="Graffiti-buttons" class="fieldcontain ui-grid-a">
<div class="ui-block-a">
<input type="submit" name="record" id="373_record" value="Save" />
</div>
<div class="ui-block-b">
<input type="button" name="cancel" id="373_cancel" value="Cancel" />
</div>
</div>
</form>